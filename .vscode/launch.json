{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "CI-TEST",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/tests/test",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "TARGET_TEST_PROG",
                    "value": "${workspaceFolder}/tests/test"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Formmater",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "KILLER-DEBUG Compile",
            "miDebuggerPath": "${workspaceFolder}/scripts/gdb",
        },
        {
            "name": "FIO",
            "type": "cppdbg",
            "request": "launch",
            "program": "/path/to/fio",
            "args": [
                "-filename=\\a",
                "-fallocate=none",
                "-direct=0",
                "-iodepth 1",
                "-rw=write",
                "-ioengine=sync",
                "-bs=4K",
                "-size=128k",
                "-name=write"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Formmater",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "KILLER-DEBUG Compile",
            "miDebuggerPath": "${workspaceFolder}/scripts/gdb",
        }
    ]
}